<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="temporal.worker"
                class="\Buyanov\SymfonyTemporalWorker\Temporal\Worker\TemporalWorker"
                public="false"
        >
            <argument type="service" id="service_container" />
        </service>
        <service
                id="temporal.client.factory"
                class="\Temporal\Client\WorkflowClientInterface"
                public="false"
        >
            <argument type="string"/> <!-- will be filled in with dsn dynamically -->
            <argument type="string"/> <!-- will be filled in with namespace dynamically -->
        </service>
        <service
                id="temporal.worker.command"
                class="\Buyanov\SymfonyTemporalWorker\Command\TemporalWorkerCommand"
                public="false"
        >
            <argument type="service" id="temporal.worker"/>
            <tag name="console.command"/>
        </service>
    </services>
</container>
